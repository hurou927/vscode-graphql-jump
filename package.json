{
  "name": "graphql-jump",
  "repository": {
    "type": "git",
    "url": "https://github.com/hurou927/vscode-graphql-jump"
  },
  "version": "1.0.10",
  "description": "Jump to GraphQL type definitions from TypeScript files",
  "author": "",
  "type": "commonjs",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "bump:patch": "./scripts/bump-version.sh patch",
    "bump:minor": "./scripts/bump-version.sh minor",
    "bump:major": "./scripts/bump-version.sh major"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.99.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "vscode": "^1.99.3"
  },
  "displayName": "GraphQL Jump",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:graphql-jump.goGraphql",
    "onCommand:graphql-jump.goGraphqlCurrentWord",
    "onCommand:graphql-jump.goGraphqlInPane",
    "onCommand:graphql-jump.goGraphqlCurrentWordInPane"
  ],
  "contributes": {
    "commands": [
      {
        "command": "graphql-jump.goGraphql",
        "title": "Go to GraphQL Definition"
      },
      {
        "command": "graphql-jump.goGraphqlCurrentWord",
        "title": "Go to GraphQL Definition (Current Word)"
      },
      {
        "command": "graphql-jump.goGraphqlInPane",
        "title": "Go to GraphQL Definition in Pane"
      },
      {
        "command": "graphql-jump.goGraphqlCurrentWordInPane",
        "title": "Go to GraphQL Definition in Pane (Current Word)"
      }
    ],
    "keybindings": [
      {
        "command": "graphql-jump.goGraphqlCurrentWord",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g",
        "when": "editorTextFocus"
      },
      {
        "command": "graphql-jump.goGraphqlCurrentWordInPane",
        "key": "ctrl+alt+shift+g",
        "mac": "cmd+option+shift+g",
        "when": "editorTextFocus"
      }
    ]
  }
}
